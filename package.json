{
  "name": "race-bot",
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "ttsc",
    "start": "node dist/src/index.js",
    "dev": "export $(cat .env | xargs) && tsc-watch --build --incremental --noClear --compiler ttypescript/bin/tsc --onSuccess \"node ./dist/src/index.js\"",
    "format": "prettier --write \"{src,test}/**/*.ts\" ",
    "lint": "eslint --fix \"{test,src}/**/*.{js,ts}\"",
    "test": "jest --verbose --runInBand --coverage",
    "migration:generate": "bash ./scripts/generate-migration.sh"
  },
  "dependencies": {
    "@types/uuid": "^8.3.1",
    "axios": "^0.21.1",
    "cron": "^1.8.2",
    "discord.js": "^13.0.1",
    "joi": "^17.4.2",
    "lock": "^1.1.0",
    "moment": "^2.29.1",
    "mysql2": "^2.3.0",
    "pino": "^6.13.0",
    "pino-pretty": "^5.1.3",
    "redis": "^3.1.2",
    "tsc-watch": "^4.4.0",
    "typeorm": "^0.2.36",
    "uuid": "^8.3.2",
    "wtfnode": "^0.9.1"
  },
  "devDependencies": {
    "@types/cron": "^1.7.3",
    "@types/jest": "^27.0.0",
    "@types/node": "16.4.13",
    "@types/pino": "^6.3.11",
    "@types/redis": "^2.8.31",
    "@types/ts-nameof": "^4.2.1",
    "@types/wtfnode": "^0.7.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^1.1.3",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.5",
    "ts-nameof": "^5.0.0",
    "ts-node": "^10.2.0",
    "ts-transformer-keys": "^0.4.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.5"
  }
