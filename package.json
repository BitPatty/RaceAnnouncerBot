{
  "name": "race-bot",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "ttsc",
    "start": "node dist/src/index.js",
    "develop": "export $(cat .env | xargs) && tsc-watch --build --incremental --noClear --compiler ttypescript/bin/tsc --onSuccess \"node ./dist/src/index.js\"",
    "format": "prettier --write \"{src,test}/**/*.ts\" ",
    "lint": "eslint --fix \"{test,src}/**/*.{js,ts}\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "discord.js": "^12.5.3",
    "joi": "^17.4.0",
    "lock": "^1.1.0",
    "mysql2": "^2.2.5",
    "tsc-watch": "^4.4.0",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@types/node": "^15.12.5",
    "@types/ts-nameof": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "jest": "^27.0.5",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-nameof": "^5.0.0",
    "ts-transformer-keys": "^0.4.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.3.4"
  }
}
